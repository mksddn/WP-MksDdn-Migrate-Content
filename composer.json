{
  "name": "mksddn/wp-mksddn-migrate-content",
  "description": "MksDdn Migrate Content plugin dev tooling",
  "type": "project",
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "wp-coding-standards/wpcs": "^3.0",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "scripts": {
    "lint": "phpcs -q",
    "lint:report": "phpcs -q --report=full"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
